{
  "name": "sortire",
  "description": "A collection of popular sorting algorithms for JavaScript",
  "version": "0.0.6",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "typings": "dist/index.d.ts",
  "scripts": {
    "dev": "rollup -c -w",
    "build": "rimraf dist && cross-env NODE_ENV=production rollup -c",
    "format": "rome format . --write",
    "format:check": "rome format .",
    "lint": "rome check .",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "devDependencies": {
    "@vitest/coverage-c8": "^0.27.0",
    "cross-env": "^7.0.3",
    "esbuild": "^0.16.16",
    "rimraf": "^3.0.2",
    "rollup": "^3.9.1",
    "rollup-plugin-dts": "^5.1.1",
    "rollup-plugin-esbuild": "^5.0.0",
    "rome": "^11.0.0",
    "typescript": "^4.9.4",
    "vitest": "^0.27.0"
  },
  "files": [
    "dist"
  ],
  "license": "MIT",
  "author": "Kevin Hermawan",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kevinhermawan/sortire.git"
  },
  "bugs": {
    "url": "https://github.com/kevinhermawan/sortire/issues"
  },
  "homepage": "https://github.com/kevinhermawan/sortire#readme",
  "keywords": [
    "array",
    "algorithms",
    "sorting-algorithms"
  ]
}
