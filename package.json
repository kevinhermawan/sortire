{
  "type": "module",
  "name": "sortire",
  "description": "A collection of popular sorting algorithms for JavaScript",
  "version": "1.0.2",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "dev": "rollup -c -w",
    "build": "rimraf dist && rollup -c",
    "format": "rome format . --write",
    "format:check": "rome format .",
    "lint": "rome check .",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "devDependencies": {
    "@vitest/coverage-v8": "^0.34.1",
    "esbuild": "^0.19.1",
    "rimraf": "^5.0.1",
    "rollup": "^3.28.0",
    "rollup-plugin-dts": "^5.3.1",
    "rollup-plugin-esbuild": "^5.0.0",
    "rome": "^12.1.3",
    "typescript": "^5.1.6",
    "vitest": "^0.34.1"
  },
  "files": [
    "dist"
  ],
  "license": "MIT",
  "author": "Kevin Hermawan",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kevinhermawan/sortire.git"
  },
  "bugs": {
    "url": "https://github.com/kevinhermawan/sortire/issues"
  },
  "homepage": "https://github.com/kevinhermawan/sortire#readme",
  "keywords": [
    "array",
    "algorithms",
    "sorting-algorithms"
  ]
}
